# Telegram Bot Token (required - get from @BotFather)
BOT_TOKEN=your_telegram_bot_token_here

# X1 Blockchain RPC URL
X1_RPC_URL=https://rpc.mainnet.x1.xyz

# Polling interval in milliseconds (default: 15000 = 15 seconds)
